#include <stdio.h>
int main() 
{
    int i, n, flag;
    printf("Enter a number: ");
    scanf("%d", &n);
   if (n < 2) {
        printf("The given number is not prime.\n");
        return 0;
    }
    flag = 0;
    for (i = 2; i < n; i++) 
    {
        if (n % i == 0) 
        {
            flag = 1;
            break;
        }
    }
   if (flag == 1) 
    {
        printf("The given number is not prime.\n");
    } 
    else 
    {
        printf("The given number is prime.\n");
    }

    return 0;
}
