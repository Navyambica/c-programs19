#include<stdio.h>
#include<string.h>
char string[20],
     string2[20];
int main() {
      
    printf("enter your name:");
    fgets(string,sizeof(string),stdin);
    printf("enter your last name:");
    fgets(string2,sizeof(string2),stdin);
     printf("the full name is %s\t",strcat(string,string2));
  
}
