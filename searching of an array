#include<stdio.h>
#include<string.h>

int main()
{
    
int i,n;
char a,arr[45];
printf("how many chracters");
scanf("%d",&n);
for(i=0;i<n;i++)
{
    printf("enter the character\n");
    scanf("%s", &arr[i]);
}
    printf("which character need to check ");
    scanf("%s",&a);

    if(arr[i]=a)
    {
        printf("it is present");
    }
    else
    {
        printf("not present");
    }
}
